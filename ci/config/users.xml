<?xml version="1.0"?>
<clickhouse>
    <profiles>
        <default>
            <max_memory_usage>8000000000</max_memory_usage>
            <serialized_index_segment_max_byte>500000000</serialized_index_segment_max_byte>
            <load_balancing>random</load_balancing>
            <log_queries>1</log_queries>
            <!-- <mutations_sync>0</mutations_sync> -->
            <!-- <replication_alter_partitions_sync>1</replication_alter_partitions_sync> -->
            <!-- <insert_distributed_sync>1</insert_distributed_sync> -->
            <optimize_move_to_prewhere>0</optimize_move_to_prewhere>
            <max_query_size>262144000</max_query_size>
            <connect_timeout_with_failover_ms>5000</connect_timeout_with_failover_ms>
            <distributed_directory_monitor_batch_inserts>1</distributed_directory_monitor_batch_inserts>
            <distributed_product_mode>global</distributed_product_mode>
            <join_use_nulls>1</join_use_nulls>
            <prefer_global_in_and_join>1</prefer_global_in_and_join>
            <readonly>0</readonly>
            <distributed_aggregation_memory_efficient>1</distributed_aggregation_memory_efficient>
            <parallel_view_processing>1</parallel_view_processing>
            <vector_index_cache_size>4</vector_index_cache_size>
            <incremental_build_index_ratio>0.4</incremental_build_index_ratio>
    	    <allow_experimental_lightweight_delete>true</allow_experimental_lightweight_delete>
            <mutations_sync>1</mutations_sync>
	    <log_query_threads>0</log_query_threads>
	    <log_queries_cut_to_length>1000</log_queries_cut_to_length>
	    <!-- <atomic_insert>false</atomic_insert> -->
        </default>
    </profiles>

    <users>
        <default>
            <password></password>
            <networks>
		<ip>::/0</ip>    
		<ip>::1</ip>
                <ip>127.0.0.1</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>

    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</clickhouse>
